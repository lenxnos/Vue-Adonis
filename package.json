{
  "name": "adonis-fullstack-app",
  "version": "4.1.0",
  "adonis-version": "4.1.0",
  "description": "The fullstack application boilerplate for Adonisjs",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "test": "node ace test",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --config config/webpack/client.config.js --progress --hide-modules",
    "build:server": "webpack --config config/webpack/server.config.js --progress --hide-modules",
    "asset-dev": "nodemon --watch app --watch bootstrap --watch config --watch .env server.js"
  },
  "keywords": [
    "adonisjs",
    "adonis-app"
  ],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/auth": "^3.0.7",
    "@adonisjs/bodyparser": "^2.0.5",
    "@adonisjs/cors": "^1.0.7",
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/framework": "^5.0.9",
    "@adonisjs/ignitor": "^2.0.8",
    "@adonisjs/lucid": "^6.1.3",
    "@adonisjs/persona": "^1.0.5",
    "@adonisjs/session": "^1.0.27",
    "@adonisjs/shield": "^1.0.8",
    "@adonisjs/validator": "^5.0.6",
    "@babel/preset-env": "^7.3.1",
    "es6-promise": "^4.0.5",
    "es6-promisify": "^5.0.0",
    "pg": "^7.6.1",
    "vue": "^2.1.7",
    "vue-router": "^2.1.1",
    "vue-server-renderer": "^2.1.7",
    "vue-template-compiler": "^2.1.7",
    "vuex": "^2.1.0",
    "vuex-router-sync": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "autoprefixer": "^9.4.6",
    "babel-loader": "^8.0.5",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-2": "^6.11.0",
    "clean-webpack-plugin": "^0.1.13",
    "cross-env": "^3.1.3",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "file-loader": "^0.9.0",
    "nodemon": "^1.11.0",
    "url-loader": "^0.5.7",
    "vue-loader": "^10.0.2",
    "webpack": "^2.1.0-beta.26",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "autoload": {
    "App": "./app"
  }
}
